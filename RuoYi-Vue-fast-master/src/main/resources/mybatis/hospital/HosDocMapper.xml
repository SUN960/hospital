<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.project.hospital.mapper.HosDocMapper">
    
    <resultMap type="HosDoc" id="HosDocResult">
        <result property="docId"    column="doc_id"    />
        <result property="docName"    column="doc_name"    />
        <result property="sex"    column="sex"    />
        <result property="birthTime"    column="birth_time"    />
        <result property="email"    column="email"    />
        <result property="phonenumber"    column="phonenumber"    />
        <result property="adoId"    column="ado_id"    />
        <result property="orderNum"    column="order_num"    />
        <result property="status"    column="status"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectHosDocVo">
        select doc_id, doc_name, sex, birth_time, email, phonenumber, ado_id, order_num, status, create_by, create_time, update_by, update_time, remark from hos_doc
    </sql>

    <select id="selectHosDocList" parameterType="HosDoc" resultMap="HosDocResult">
        <include refid="selectHosDocVo"/>
        <where>  
            <if test="docName != null  and docName != ''"> and doc_name like concat('%', #{docName}, '%')</if>
            <if test="sex != null  and sex != ''"> and sex = #{sex}</if>
            <if test="email != null  and email != ''"> and email = #{email}</if>
            <if test="phonenumber != null  and phonenumber != ''"> and phonenumber = #{phonenumber}</if>
            <if test="adoId != null "> and ado_id = #{adoId}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectHosDocById" parameterType="Long" resultMap="HosDocResult">
        <include refid="selectHosDocVo"/>
        where doc_id = #{docId}
    </select>
        
    <insert id="insertHosDoc" parameterType="HosDoc" useGeneratedKeys="true" keyProperty="docId">
        insert into hos_doc
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="docName != null and docName != ''">doc_name,</if>
            <if test="sex != null">sex,</if>
            <if test="birthTime != null">birth_time,</if>
            <if test="email != null">email,</if>
            <if test="phonenumber != null">phonenumber,</if>
            <if test="adoId != null">ado_id,</if>
            <if test="orderNum != null">order_num,</if>
            <if test="status != null">status,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="docName != null and docName != ''">#{docName},</if>
            <if test="sex != null">#{sex},</if>
            <if test="birthTime != null">#{birthTime},</if>
            <if test="email != null">#{email},</if>
            <if test="phonenumber != null">#{phonenumber},</if>
            <if test="adoId != null">#{adoId},</if>
            <if test="orderNum != null">#{orderNum},</if>
            <if test="status != null">#{status},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateHosDoc" parameterType="HosDoc">
        update hos_doc
        <trim prefix="SET" suffixOverrides=",">
            <if test="docName != null and docName != ''">doc_name = #{docName},</if>
            <if test="sex != null">sex = #{sex},</if>
            <if test="birthTime != null">birth_time = #{birthTime},</if>
            <if test="email != null">email = #{email},</if>
            <if test="phonenumber != null">phonenumber = #{phonenumber},</if>
            <if test="adoId != null">ado_id = #{adoId},</if>
            <if test="orderNum != null">order_num = #{orderNum},</if>
            <if test="status != null">status = #{status},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where doc_id = #{docId}
    </update>

    <delete id="deleteHosDocById" parameterType="Long">
        delete from hos_doc where doc_id = #{docId}
    </delete>

    <delete id="deleteHosDocByIds" parameterType="String">
        delete from hos_doc where doc_id in 
        <foreach item="docId" collection="array" open="(" separator="," close=")">
            #{docId}
        </foreach>
    </delete>
    
</mapper>